{
  "name": "big-appetite-os",
  "version": "1.0.0",
  "description": "Big Appetite OS - Self-aware, learning business operating system with adaptive creative feedback loop",
  "main": "main/run_cycle.ts",
  "type": "module",
  "scripts": {
    "test": "node scripts/test-connection.js",
    "stage1": "node scripts/run-stage1.js",
    "setup": "node scripts/setup-env.js",
    "dev": "./node_modules/.bin/tsx main/run_cycle.ts",
    "cycle": "./node_modules/.bin/tsx main/run_cycle.ts",
    "observation": "tsx main/run_cycle.ts observation",
    "adjustment": "tsx main/run_cycle.ts adjustment",
    "copy": "tsx main/run_cycle.ts copy",
    "asset": "tsx main/run_cycle.ts asset",
    "feedback": "tsx main/run_cycle.ts feedback",
    "build": "tsc",
    "start": "node dist/main/run_cycle.js",
    "realtime": "node start_realtime_system.js",
    "monitor": "node monitor_system.js",
    "realtime:start": "node start_realtime_system.js",
    "realtime:monitor": "node monitor_system.js",
    "quantum:start": "npx tsx integration/quantum-agent-scheduler.ts",
    "quantum:trigger": "npx tsx integration/quantum-agent-scheduler.ts trigger",
    "quantum:process": "npx tsx integration/quantum-agent-scheduler.ts process",
    "quantum:stats": "tsx -e \"import {getProcessingStats} from './integration/agent-integration-service'; getProcessingStats().then(console.log)\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "ai",
    "business-intelligence",
    "customer-analytics",
    "supabase",
    "adaptive-creative",
    "bayesian-learning",
    "markov-processes"
  ],
